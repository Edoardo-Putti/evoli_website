extends layout 

block content 
    body(class="feedback")
        header
            nav#navbarCont(class="d-flex  flex-md-row  navbar navbar-dark navbar-expand-md p-2 px-md-4 ")
                button(class="navbar-toggler " type="button" data-toggle="collapse" data-target="#Navbar")
                    span(class="navbar-toggler-icon")
                div(class="d-flex d-none d-lg-block" style="color: whitesmoke; text-align: center; font-size:calc(10px + .6vw); margin-right: 20px;")
                    p#teachersName(style="color: whitesmoke; text-align: center; font-size: calc(10px + .8vw); margin: auto;")
                        i(class="fa fa-user")
                        | Logged in as: 
                        != name
                #Navbar(class="collapse navbar-collapse justify-content-end")
                    ul(class="navbar-nav ml-auto" style="margin-left: 0!important;")
                        li(class="nav-item ")
                            a(class="nav-link" href="/")
                                span(class="fa fa-home fa-lg")
                                | Home
                        li(class="nav-item")
                            a(class="nav-link" href="/teacher")
                                span
                                    img#teachersLogo(src="../images/chalkboard.png" height="30px" width="30px")
                                | My videos
                        li(class="nav-item active")
                            a(class="nav-link")
                                span(class="fa fa-bar-chart fa-lg")
                                | Statistics
                a(role="button" class="btn  btn-logOut  rounded-pill" href='../teacher/logout')
                    | Log
                    | Out
        div(class="row d-flex justify-content-center align-items-center" style="margin-bottom: 10px; margin-top: 10px") 
            h4 General overview between the two groups
        div(class="row d-flex justify-content-center align-items-center" )
            div(class="col-lg-3 col-md-6 col-xs-12")
                canvas#compareUnderstanding
            div(class="col-lg-3 col-md-6 col-xs-12")
                canvas#compareAppreciation
            div(class="col-lg-3 col-md-6 col-xs-12")
                canvas#compareReactions
            div(class="col-lg-3 col-md-6 col-xs-12")
                canvas#compareStudents
        //div(class="row d-flex justify-content-center align-items-center" style="margin-bottom: 10px; margin-top: 10px; border-bottom: 2px solid #243d70") 
        div(class="row d-flex justify-content-center")
            div(class="col-lg-6 col-md-12" style="border: 2px solid #243d70;")
                div(class="row d-flex justify-content-center" style="margin-top: 10px;")
                    div(class="col-12 d-flex justify-content-center"  style="margin-top: 10px; margin-bottom: 10px;")
                        h4#v1 Focus on first Group that has
                    div(class="col-12 d-flex justify-content-center" style="margin-top: 10px; margin-bottom: 10px;")
                         h4#u1 With an Avg. Understanding of: 
                    div(class="col-6")
                        canvas#chartUnderstanding
                    div(class="col-6")
                        canvas#chartAppreciation
                div(class="row d-flex justify-content-center" )
                    div(class="col-12 d-flex justify-content-center"  style="margin-top: 10px; margin-bottom: 10px;")
                        h4#r1 The videos received a total of 
                    div(class="col-10")
                        canvas#chartReactions
                div(class="row d-flex justify-content-center"  )
                    div(class="col-12 d-flex justify-content-center" style="margin-top: 10px; margin-bottom: 10px;")
                        h4#s1 The videos were watched by 
                    div(class="col-10")
                        canvas#chartStudents
            div(class="col-lg-6 col-md-12"  style="border: 2px solid #243d70; ")
                div(class="row d-flex justify-content-center" style="margin-top: 10px;")
                    div(class="col-12 d-flex justify-content-center"  style="margin-top: 10px; margin-bottom: 10px;")
                        h4#v2 Focus on Second Group that has:
                    div(class="col-12 d-flex justify-content-center" style="margin-top: 10px; margin-bottom: 10px;")
                         h4#u2 With an Avg. Understanding of:  
                    div(class="col-6")
                        canvas#chartUnderstanding1
                    div(class="col-6")
                        canvas#chartAppreciation1
                div(class="row d-flex justify-content-center" )
                    div(class="col-12 d-flex justify-content-center"  style="margin-top: 10px; margin-bottom: 10px;")
                        h4#r2 The videos received a total of 
                    div(class="col-10")
                        canvas#chartReactions1
                div(class="row d-flex justify-content-center" )
                    div(class="col-12 d-flex justify-content-center"  style="margin-top: 10px; margin-bottom: 10px;")
                        h4#s2 The videos were watched by
                    div(class="col-10")
                        canvas#chartStudents1
        #loading
            img(src="../images/Ellipsis.gif" alt="loading")
    script(src="../javascripts/compare.js") 
    script(src="../javascripts/node_modules/patternomaly/dist/patternomaly.js") 
    script(src="../javascripts/node_modules/html2canvas/dist/html2canvas.js")
    script(src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js")  
    script(src="https://cdn.jsdelivr.net/npm/chart.js")
    script(src="https://cdn.jsdelivr.net/npm/hammerjs@2.0.8")
    script(src="https://cdn.jsdelivr.net/npm/chartjs-plugin-zoom@next")